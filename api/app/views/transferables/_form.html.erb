<%= admin_form_for @transferable do |form| %>
  <%= form.error_notification %>
  <%= form.hidden_field :game_id %>
  <%= form.input :identifier %>
  <%= form.input :name %>
  <%= form.input :value %>
  <%= form.image_input :image %>
  <%= form.buttons cancel_path: game_path(form.object.game) %>
<% end %>
